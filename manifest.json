{
  "manifest_version": 2,

  "name": "Video Replay",
  "description": "This extension adds a loop function to html5 videos.",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["src/jquery-2.1.4.min.js", "src/loop.js"],
      "run_at": "document_end"
    }
  ]
}